{
  "name": "gsuite-signature-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @gsuite/frontend dev",
    "dev:no-seed": "yarn workspace @gsuite/frontend dev:no-seed",
    "dev:all": "yarn workspace @gsuite/frontend dev:all",
    "build": "yarn workspaces foreach --all -p run build",
    "lint": "yarn workspaces foreach --all -p run lint",
    "test": "yarn workspaces foreach --all -p run test",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tanstack/router-plugin": "^1.119.0",
    "@types/add": "^2.0.3",
    "add": "^2.0.6",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "husky": "^9.1.7",
    "typescript-eslint": "^8.32.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.8.1"
}
